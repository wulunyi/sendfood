<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.fenghuo.domain.Default_order">
  		<insert id="add" parameterType="Default_order"  keyProperty="default_order_id" useGeneratedKeys="true">
  			insert into default_order(school_id,default_order_name,default_order_status)
  		values (#{school_id},#{default_order_name},#{default_order_status})
  		</insert>
  		
  </mapper>